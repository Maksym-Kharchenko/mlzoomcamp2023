import pickle

def load(filename: str):
    with open(filename, 'rb') as f_in:
        return pickle.load(f_in)

dv = load('dv.bin')
model = load('model1.bin')

clients = [{"reports": 0, "share": 0.001694, "expenditure": 0.12, "owner": "yes"},
          {"reports": 2, "share": 0.5, "expenditure": 0.8, "owner": "no"}]

for client in clients:
    X = dv.transform([client])  # Transform each client's data separately
    y_pred = model.predict_proba(X)[0, 1]
    print(f"Predicted probability for client: {y_pred}")
